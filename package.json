{
  "name": "terraso-web-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@changey/react-leaflet-markercluster": "^4.0.0-rc1",
    "@hookform/resolvers": "^3.1.0",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.132",
    "@mui/material": "^5.13.4",
    "@mui/x-data-grid": "^6.4.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@rollbar/react": "^0.11.1",
    "@turf/bbox": "^6.5.0",
    "@turf/center": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "canvg": "^4.0.1",
    "filesize": "^10.0.7",
    "get-video-id": "^3.6.5",
    "history": "^5.3.0",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.3",
    "leaflet-draw": "^1.0.4",
    "leaflet-easyprint": "^2.1.9",
    "leaflet-geosearch": "^3.8.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.14.1",
    "notistack": "^3.0.1",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-avatar-editor": "^13.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.9",
    "react-helmet-async": "^1.3.0",
    "react-highlight-words": "^0.20.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.3.1",
    "react-leaflet": "^4.2.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "scrollama": "^3.2.0",
    "slate-history": "^0.93.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.90.0",
    "use-debounce": "^9.0.4",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.1",
    "world-countries": "^4.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.10/xlsx-0.18.10.tgz",
    "yup": "^1.1.1"
  },
  "overrides": {
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "start": "npm run generate-types -- -w -s & react-scripts start",
    "generate-types": "npx graphql-code-generator",
    "lint-js": "eslint ./ --fix --ext .js --ext .ts --ext .tsx --max-warnings 0 --ignore-path .gitignore",
    "check-ts": "npm run generate-types && tsc --noEmit",
    "lint-css": "stylelint --fix --config=.stylelintrc '**/*.css'",
    "build": "npm run generate-types && react-scripts build",
    "build-serve": "serve -s build",
    "test": "npm run generate-types -- -w -s & react-scripts test --transformIgnorePatterns \"node_modules/(?!react-leaflet)/\"",
    "test-debug": "npm run generate-types && react-scripts --inspect-brk test --runInBand --no-cache --transformIgnorePatterns \"node_modules/(?!react-leaflet)/\"",
    "test-coverage": "npm run generate-types && react-scripts test --coverage --changedSince=origin/main --watchAll=false --transformIgnorePatterns \"node_modules/(?!react-leaflet)/\"",
    "localization-to-po": "node scripts/localization/transform-to.js po",
    "localization-to-json": "node scripts/localization/transform-to.js json",
    "localization-check-missing": "node scripts/localization/missing-check.js",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.1.0",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^4.2.1",
    "flat": "^5.0.2",
    "i18next-conv": "^13.1.1",
    "jest-axe": "^7.0.1",
    "plausible-tracker": "^0.3.8",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.0",
    "stylelint": "^15.6.2",
    "stylelint-prettier": "^3.0.0",
    "terraso-backend": "techmatters/terraso-backend#d1a9923",
    "typescript": "^5.0.4"
  },
  "jest": {
    "transform": {},
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/terrasoBackend/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "graphql": {
    "schema": "node_modules/terraso-backend/terraso_backend/apps/graphql/schema/schema.graphql",
    "documents": "src/**/*.{js,ts,tsx}",
    "extensions": {
      "codegen": {
        "generates": {
          "src/terrasoApi/gql/": {
            "preset": "client",
            "config": {
              "documentMode": "string",
              "skipTypename": true,
              "enumsAsTypes": true,
              "dedupeFragments": true
            },
            "presetConfig": {
              "fragmentMasking": false
            }
          }
        }
      }
    }
  }
}
