  "private": true,
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.3",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.59",
    "@mui/material": "^5.1.0",
    "@mui/x-data-grid": "^5.2.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "history": "^5.1.0",
    "i18next": "^21.5.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "lodash": "^4.17.21",
    "notistack": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.1",
    "react-i18next": "^11.14.2",
    "react-leaflet": "^3.2.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.1",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint ./ --fix --ext .js --max-warnings 0 --ignore-pattern node_modules/ --ignore-pattern build/",
    "build": "react-scripts build",
    "build-serve": "serve -s build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage --changedSince=origin/main --watchAll=false",
    "localization-to-po": "node scripts/localization/transform-to.js po",
    "localization-to-json": "node scripts/localization/transform-to.js json",
    "localization-check-missing": "node scripts/localization/missing-check.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "jsx-a11y"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "standard",
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "react/jsx-indent": [2, 2, {"checkAttributes": true, "indentLogicalExpressions": true}],
      "prefer-promise-reject-errors": "off",
      "multiline-ternary": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-standard": "^16.0.3",
    "flat": "^5.0.2",
    "i18next-conv": "^11.0.1",
    "react-scripts": "^4.0.3",
    "serve": "^13.0.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
