{
  "name": "terraso-web-client",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "src/config.ts"
  ],
  "dependencies": {
    "@graphql-codegen/client-preset": "^4.0.1",
    "@hookform/resolvers": "^3.1.1",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@mui/icons-material": "^5.14.1",
    "@mui/lab": "^5.0.0-alpha.137",
    "@mui/material": "^5.14.1",
    "@mui/x-data-grid": "^6.10.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@turf/bbox": "^6.5.0",
    "@turf/center": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "canvg": "^4.0.1",
    "filesize": "^10.0.7",
    "get-video-id": "^3.6.5",
    "i18next": "^23.2.11",
    "i18next-browser-languagedetector": "^7.1.0",
    "js-cookie": "^3.0.5",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet-geosearch": "^3.8.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.15.0",
    "notistack": "^3.0.1",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-avatar-editor": "^13.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.10",
    "react-helmet-async": "^1.3.0",
    "react-highlight-words": "^0.20.0",
    "react-hook-form": "^7.45.2",
    "react-i18next": "^13.0.2",
    "react-leaflet": "^4.2.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.2",
    "scrollama": "^3.2.0",
    "slate-history": "^0.93.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.97.2",
    "terraso-client-shared": "github:techmatters/terraso-client-shared#3072075",
    "use-debounce": "^9.0.4",
    "uuid": "^9.0.0",
    "web-vitals": "^3.4.0",
    "world-countries": "^4.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.10/xlsx-0.18.10.tgz",
    "yup": "^1.2.0"
  },
  "overrides": {
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "start": "npm run generate-types -- -w -s & react-scripts start",
    "generate-types": "npx graphql-code-generator",
    "lint-js": "eslint ./ --ext .js --ext .ts --ext .tsx --max-warnings 0 --ignore-path .gitignore",
    "format-js": "npm run lint-js -- --fix",
    "check-ts": "npm run generate-types && tsc --noEmit",
    "lint-css": "stylelint --config=.stylelintrc '**/*.css'",
    "format-css": "npm run lint-css -- --fix",
    "build": "npm run generate-types && react-scripts build",
    "build-serve": "serve -s build",
    "test": "npm run generate-types && react-scripts test --transformIgnorePatterns \"node_modules/(?!react-leaflet)/\"",
    "test-debug": "npm run generate-types && react-scripts --inspect-brk test --runInBand --no-cache --transformIgnorePatterns \"node_modules/(?!react-leaflet)/\"",
    "test-coverage": "npm run generate-types && react-scripts test --coverage --changedSince=origin/main --watchAll=false --transformIgnorePatterns \"node_modules/(?!react-leaflet)/\"",
    "localization-to-po": "node scripts/localization/transform-to.js po",
    "localization-to-json": "node scripts/localization/transform-to.js json",
    "localization-check-missing": "node scripts/localization/missing-check.js",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^5.0.0",
    "flat": "^5.0.2",
    "i18next-conv": "^14.0.0",
    "jest-axe": "^8.0.0",
    "plausible-tracker": "^0.3.8",
    "prettier": "^3.0.0",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.0",
    "stylelint": "^15.10.2",
    "stylelint-prettier": "^4.0.0"
  },
  "jest": {
    "transform": {},
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/terrasoBackend/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "graphql": {
    "schema": "node_modules/terraso-backend/terraso_backend/apps/graphql/schema/schema.graphql",
    "documents": [
      "src/**/*.{js,ts,tsx}",
      "node_modules/terraso-client-shared/src/**/*Fragments.ts"
    ],
    "extensions": {
      "codegen": {
        "generates": {
          "src/terrasoApi/shared/graphqlSchema/": {
            "preset": "client",
            "config": {
              "documentMode": "string",
              "skipTypename": true,
              "enumsAsTypes": true,
              "dedupeFragments": true
            },
            "presetConfig": {
              "fragmentMasking": false
            }
          }
        }
      }
    }
  }
}
